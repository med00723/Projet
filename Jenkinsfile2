
pipeline {

  environment {
    dockerimagename = "med00723/backend"
    dockerImage = ""
  }

  agent any

  stages {

    stage('Checkout Source') {
      steps {
        git 'https://github.com/med00723/Projet.git'
      }
    }

    stage('Build image'){
        steps {
            dir('/var/lib/jenkins/workspace/new/'){
                sh 'npm install'
              }
        }
    }


  }

}



