
pipeline {

  environment {
    dockerimagename = "med00723/backend"
    dockerImage = ""
  }

  agent any

  stages {

    stage('Checkout Source') {
      steps {
        git 'https://github.com/med00723/Projet.git'
      }
    }

    stage('Build docker image'){
        steps {  
            
    //      
            sh 'npm install "./msign/backend"'
        }
    }


  }

}
